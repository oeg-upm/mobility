    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#duration -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#duration"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#string -->


    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string">
        <owl:equivalentClass rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2004/02/skos/core#inScheme -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#inScheme"/>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#accessedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#accessedBy">
        <rdfs:comment xml:lang="en">This property sets the relationship between Site and Entrance</rdfs:comment>
        <rdfs:label xml:lang="en">accessed By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#accessibilityAssessment -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#accessibilityAssessment">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#AccessibilityAssessment"/>
        <rdfs:comment xml:lang="en">This property is used to relate a site element, a facility, etc., with a specific accessibility assessment. In some of the UML models of Transmodel this property is name as characerisedBy</rdfs:comment>
        <rdfs:label xml:lang="en">accessibility Assessment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#accommodationFacility -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#accommodationFacility">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#facilityType"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#Accommodation"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property restricts the type of facility to one specific for accommodation from the SKOS codelist for types of facilities, including, for example, values like seating, sleeper, couchette, special seating, reclining seats, standing, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">accommodation facility</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#assignedTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#assignedTo">
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#WaitingRoomEquipment"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/fares#ClassOfUse"/>
        <rdfs:comment xml:lang="en">This property specifies the assignment of a Waiting Room Equipment to a class of usage according to the fare that has been paid, as defined in the ontology about fares</rdfs:comment>
        <rdfs:label xml:lang="en">assigned to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#availableAt -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#availableAt">
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#SiteFacilitySet"/>
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#Site"/>
        <rdfs:comment xml:lang="en">This property relates a Site Facility Set with a Site</rdfs:comment>
        <rdfs:label xml:lang="en">available At</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#boardingPermission -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#boardingPermission">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#facilityType"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#OnboardStay"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property describes the types of boarding permissions allowed for an Onboard Stay from the SKOS codelist for types of facilities, including normal, early boarding, late alighting, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">boarding permission</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#comprises -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#comprises">
        <rdfs:comment xml:lang="en">General property for all types of containment and aggregation</rdfs:comment>
        <rdfs:label xml:lang="en">comprises</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#comprisesEquipment -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#comprisesEquipment">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#comprises"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#FacilitySet"/>
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#Equipment"/>
        <rdfs:comment xml:lang="en">This property is used to aggregate other types of facilities from a Facility Set, normally focused on those that are user defined and not from the predefined list.
In the Transmodel UML model this is represented as TypeOfEquipment, and in the Netex implementation the term otherFacilities is used.</rdfs:comment>
        <rdfs:label xml:lang="en">comprises equipment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#comprisesFacility -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#comprisesFacility">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#comprises"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#FacilitySet"/>
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#Facility"/>
        <rdfs:comment xml:lang="en">This property is used to aggregate all the facilities that a facility set contains
In Netex, this is expressed with the element TypeOfFacilityRef, which is used to refer to facilities, and in the Transmodel UML ontology this is simply expressed as &apos;facility&apos;</rdfs:comment>
        <rdfs:label xml:lang="en">comprises facility</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#concernedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#concernedBy">
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#FacilitySet"/>
        <rdfs:comment xml:lang="en">This property allows connecting any object with a Facility Set</rdfs:comment>
        <rdfs:label xml:lang="en">concerned By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#containing -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#containing">
        <rdfs:comment xml:lang="en">This property allows specifying that a Stop contains Quays</rdfs:comment>
        <rdfs:label xml:lang="en">containing</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#couchetteFacility -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#couchetteFacility">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#facilityType"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#Accommodation"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property restricts the type of facility to one specific for couchette from the SKOS codelist for types of facilities, including, for example, values like T2, T3, C1, C2, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">couchette facility</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#determining -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#determining">
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#AccessibilityAssessment"/>
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#AccessibilityLimitation"/>
        <rdfs:comment xml:lang="en">This property sets the relationship between Accessibility Assessment and Accessibility Limitation</rdfs:comment>
        <rdfs:label xml:lang="en">determining</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#directionOfUseType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#directionOfUseType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#equipmentType"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#AccessEquipment"/>
        <rdfs:comment xml:lang="en">Direction in which Equipment can be used. (e.g. up, down, level, one way, both way, etc.).</rdfs:comment>
        <rdfs:label xml:lang="en">direction of use</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#equipmentType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#equipmentType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#Equipment"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/EquipmentType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property is used to link an equipment with the types of equipments available in the SKOS concept scheme of types of equipments</rdfs:comment>
        <rdfs:label xml:lang="en">equipment type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#equippedWith -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#equippedWith">
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#EquipmentPlace"/>
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#Equipment"/>
        <rdfs:comment xml:lang="en">This property is used to define the equipments that are available in an equipment place</rdfs:comment>
        <rdfs:label xml:lang="en">equipped With</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#facilityType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#facilityType">
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#Facility"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property is used to link a facility with the types of facilities available in the SKOS concept scheme of types of facilities</rdfs:comment>
        <rdfs:comment xml:lang="en">facility type</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#fareClass -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#fareClass">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#facilityType"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#ServiceFacilitySet"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property relates an Onboard Stay or an Accommodation, among others, with a class of fares, whose allowed values are defined in the TransModel Common Facility Values (Figure 290, page 454)</rdfs:comment>
        <rdfs:label xml:lang="en">fare class</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#genderLimitationType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#genderLimitationType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#equipmentType"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#SanitaryEquipment"/>
        <rdfs:comment xml:lang="en">A classification for GENDER LIMITATIONSs (mainly for SANITARY EQUIPMENT, e.g. male only, female only, both).</rdfs:comment>
        <rdfs:label xml:lang="en">gender limitation type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#limitedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#limitedBy">
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#AccessibilityLimitation"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#locatedIn -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#locatedIn">
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#Equipment"/>
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#EquipmentPlace"/>
        <rdfs:comment xml:lang="en">This property is used to relate an equipment with the equipment place where it is located</rdfs:comment>
        <rdfs:label xml:lang="en">located In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#luggageLockerType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#luggageLockerType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#equipmentType"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#LuggageLockerEquipment"/>
        <rdfs:comment xml:lang="en">This property specialised the property equipentType and is applicable to Luggage Locker Equipment. Its values are narrower than the SKOS concept LuggageLockerFacilityType</rdfs:comment>
        <rdfs:label xml:lang="en">luggage locker type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#position -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#position">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/mobility/transmodel/facilities#EquipmentPlace"/>
                    <rdf:Description rdf:about="https://w3id.org/mobility/transmodel/facilities#PlaceEquipment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/mobility/transmodel/facilities#EquipmentPosition"/>
        <rdfs:comment xml:lang="en">This property is used to relate an equipment place (a collection of equipments) or a specific place equipment with the different equipment positions that they may have</rdfs:comment>
        <rdfs:label xml:lang="en">position</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#presentAt -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#presentAt">
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#ServiceFacilitySet"/>
        <rdfs:comment xml:lang="en">This property relates a Service Facility Set with a Vehicle Type, indicating that the set of service facilities comprised by the set are provided at that type of vehicle</rdfs:comment>
        <rdfs:label xml:lang="en">present at</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/mobility/transmodel/facilities#providedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/mobility/transmodel/facilities#providedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/mobility/transmodel/facilities#FacilitySet"/>
        <rdfs:comment xml:lang="es">This property is used to refer to the provider of a facility set
In the TransModel UML model and in the Netex implementation this is expressed as ProvidedByRef</rdfs:comment>
        <rdfs:label xml:lang="es">provided by</rdfs:label>
    </owl:ObjectProperty>
    


