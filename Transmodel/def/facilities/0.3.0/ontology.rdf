        <rdfs:subClassOf rdf:resource="https://w3id.org/mobility/transmodel/facilities#SiteComponent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/mobility/transmodel/facilities#equippedWith"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/mobility/transmodel/facilities#Equipment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/mobility/transmodel/facilities#position"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/mobility/transmodel/facilities#EquipmentPosition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An Equipment Place is a Site Component containing Equipment</rdfs:comment>
        <rdfs:label>Equipment Place</rdfs:label>
    </owl:Class>
    


