#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/mobility/trias/fares#bookingAgencyName
trfares:bookingAgencyName rdf:type owl:ObjectProperty ;
                          rdfs:domain trfares:BookingInfo ;
                          rdfs:range <https://w3id.org/mobility/trias/utility#InternationalText> ;
                          rdfs:comment "Property related to the name of a booking agency."@en ;
                          rdfs:label "bookingAgencyName"@en .


###  https://w3id.org/mobility/trias/fares#hasBookingInfo
trfares:hasBookingInfo rdf:type owl:ObjectProperty ;
                       rdfs:domain trfares:Ticket ;
                       rdfs:range trfares:BookingInfo ;
                       rdfs:comment "Description of booking options."@en ;
                       rdfs:label "hasBookingInfo"@en .


###  https://w3id.org/mobility/trias/fares#hasErrorMessage
trfares:hasErrorMessage rdf:type owl:ObjectProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( trfares:FaresResponse
                                                    trfares:TripFaresResult
                                                  )
                                    ] ;
                        rdfs:range trcommon:ErrorMessage ;
                        rdfs:label "hasErrorMessage"@en .


###  https://w3id.org/mobility/trias/fares#hasFareZone
trfares:hasFareZone rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain trfares:FareZoneListInArea ;
                    rdfs:range trfares:FareZone ;
                    rdfs:comment "This property represent one or more tariff zones."@en ;
                    rdfs:label "hasFareZone"@en .


###  https://w3id.org/mobility/trias/fares#hasFareZoneListInArea
trfares:hasFareZoneListInArea rdf:type owl:ObjectProperty ;
                              rdfs:domain trfares:StopFaresResult ;
                              rdfs:range trfares:FareZoneListInArea .


###  https://w3id.org/mobility/trias/fares#hasFaresResult
trfares:hasFaresResult rdf:type owl:ObjectProperty ;
                       rdfs:domain trfares:FaresResponse ;
                       rdfs:range trfares:TripFaresResult ;
                       rdfs:label "hasFaresResult"@en .


###  https://w3id.org/mobility/trias/fares#hasMultiTripFaresResult
trfares:hasMultiTripFaresResult rdf:type owl:ObjectProperty ;
                                rdfs:domain trfares:MultiTripFares ;
                                rdfs:range trfares:MultiTripFaresResult ;
                                rdfs:label "hasMultiTripFaresResult"@en .


###  https://w3id.org/mobility/trias/fares#hasParams
trfares:hasParams rdf:type owl:ObjectProperty ;
                  rdfs:comment "Parameter to filter the response contents."@en ;
                  rdfs:label "hasParams"@en .


###  https://w3id.org/mobility/trias/fares#hasStaticFaresResult
trfares:hasStaticFaresResult rdf:type owl:ObjectProperty ;
                             rdfs:domain trfares:StaticFares ;
                             rdfs:range trfares:StaticFaresResult ;
                             rdfs:label "hasStaticFaresResult"@en .


###  https://w3id.org/mobility/trias/fares#hasStaticInfoUrl
trfares:hasStaticInfoUrl rdf:type owl:ObjectProperty ;
                         rdfs:domain trfares:TripFaresResult ;
                         rdfs:range <https://w3id.org/mobility/trias/utility#WebLink> ;
                         rdfs:comment "URL for information pages."@en ;
                         rdfs:label "hasStaticInfoUrl"@en .


###  https://w3id.org/mobility/trias/fares#hasStopFaresResult
trfares:hasStopFaresResult rdf:type owl:ObjectProperty ;
                           rdfs:domain trfares:StopFares ;
                           rdfs:range trfares:StopFaresResult ;
                           rdfs:label "hasStopFaresResult"@en .


###  https://w3id.org/mobility/trias/fares#hasTariffLevelLabel
trfares:hasTariffLevelLabel rdf:type owl:ObjectProperty ;
                            rdfs:domain trfares:Ticket ;
                            rdfs:range <https://w3id.org/mobility/trias/utility#InternationalText> ;
                            rdfs:comment "Name for tariff levels in this context (example from Nürnberg “price level”, “tariff level”)"@en ;
                            rdfs:label "hasTariffLevelLabel"@en .


###  https://w3id.org/mobility/trias/fares#hasTicket
trfares:hasTicket rdf:type owl:ObjectProperty ;
                  rdfs:domain trfares:TripFaresResult ;
                  rdfs:range trfares:Ticket ;
                  rdfs:comment "Reference to tickets which are valid on this section of the trip."@en ;
                  rdfs:label "hasTicket"@en .


###  https://w3id.org/mobility/trias/fares#hasTraveller
trfares:hasTraveller rdf:type owl:ObjectProperty ;
                     rdfs:label "hasTraveller"@en .


###  https://w3id.org/mobility/trias/fares#hasTrip
trfares:hasTrip rdf:type owl:ObjectProperty ;
                rdfs:domain trfares:TripFaresRequest ;
                rdfs:range trtrips:Trip ;
                rdfs:comment "A complete trip from origin to destination"@en ;
                rdfs:label "hasTrip"@en .


###  https://w3id.org/mobility/trias/fares#hasTripFaresResult
trfares:hasTripFaresResult rdf:type owl:ObjectProperty ;
                           rdfs:domain trfares:TripFares ;
                           rdfs:range trfares:TripFaresResult ;
                           rdfs:label "hasTripFaresResult"@en .


###  https://w3id.org/mobility/trias/fares#hasValidityFareZones
trfares:hasValidityFareZones rdf:type owl:ObjectProperty ;
                             rdfs:domain trfares:Ticket ;
                             rdfs:range trfares:FareZoneListInArea ;
                             rdfs:comment "Geographic validity of the ticket stated with the help of a list of tariff zones, for which the ticket is valid."@en ;
                             rdfs:label "hasValidityFareZones"@en .


###  https://w3id.org/mobility/trias/fares#infoUrl
trfares:infoUrl rdf:type owl:ObjectProperty ;
                rdfs:domain trfares:BookingInfo ,
                            trfares:Ticket ;
                rdfs:range <https://w3id.org/mobility/trias/utility#WebLink> .


###  https://w3id.org/mobility/trias/fares#passedZones
trfares:passedZones rdf:type owl:ObjectProperty ;
                    rdfs:domain trfares:TripFaresResult ;
                    rdfs:range trfares:FareZoneListInArea ;
                    rdfs:label "This property shows if there are traversed tariff zones on this section of the trip."@en ,
                               "passedZones"@en .


###  https://w3id.org/mobility/trias/fares#saleUrl
trfares:saleUrl rdf:type owl:ObjectProperty ;
                rdfs:domain trfares:Ticket ;
                rdfs:range <https://w3id.org/mobility/trias/utility#WebLink> ;
                rdfs:comment "URL for online purchase options in order to purchase this ticket ."@en ;
                rdfs:label "saleUrl"@en .


###  https://w3id.org/mobility/trias/fares#validityAreaText
trfares:validityAreaText rdf:type owl:ObjectProperty ;
                         rdfs:domain trfares:Ticket ;
                         rdfs:range <https://w3id.org/mobility/trias/utility#InternationalText> ;
                         rdfs:comment "Description of validity zone."@en ;
                         rdfs:label "validityAreaText"@en .


###  https://w3id.org/mobility/trias/fares#validityDurationText
trfares:validityDurationText rdf:type owl:ObjectProperty ;
                             rdfs:domain trfares:Ticket ;
                             rdfs:range <https://w3id.org/mobility/trias/utility#InternationalText> ;
                             rdfs:comment "Description of validity duration."@en ;
                             rdfs:label "validityDurationText"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/identifier
<http://purl.org/dc/terms/identifier> rdf:type owl:DatatypeProperty ;
                                      rdfs:range xsd:string ;
                                      rdfs:comment "An unambiguous reference to the resource within a given context."@en ;
                                      rdfs:isDefinedBy <http://purl.org/dc/terms/> ;
                                      rdfs:label "identifier"@en .


###  https://w3id.org/mobility/trias/fares#age
trfares:age rdf:type owl:DatatypeProperty ;
            rdfs:domain trfares:FaresPassenger ;
            rdfs:range xsd:string .


###  https://w3id.org/mobility/trias/fares#bookingDeadline
trfares:bookingDeadline rdf:type owl:DatatypeProperty ;
                        rdfs:domain trfares:BookingInfo ;
                        rdfs:range xsd:dateTime ;
                        rdfs:comment "Minimum waiting time for booking before the journey begins."@en ;
                        rdfs:label "bookingDeadline"@en .


###  https://w3id.org/mobility/trias/fares#bookingUrl
trfares:bookingUrl rdf:type owl:DatatypeProperty ;
                   rdfs:domain trfares:BookingInfo ;
                   rdfs:range xsd:string ;
                   rdfs:comment "URL for online-booking."@en ;
                   rdfs:label "bookingUrl"@en .


###  https://w3id.org/mobility/trias/fares#currency
trfares:currency rdf:type owl:DatatypeProperty ;
                 rdfs:domain trfares:Ticket ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Currency code as per ISO 4217, e.g. “EUR” or “GBP”."@en ;
                 rdfs:label "currency"@en .


###  https://w3id.org/mobility/trias/fares#date
trfares:date rdf:type owl:DatatypeProperty ;
             rdfs:domain trfares:StopFaresRequest ;
             rdfs:range xsd:dateTime ;
             rdfs:comment "Date for which to retrieve fares information."@en ;
             rdfs:label "date"@en .


###  https://w3id.org/mobility/trias/fares#fareAuthorityFilter
trfares:fareAuthorityFilter rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf trfares:fareParam ;
                            rdfs:comment "Fare authority for which to retrieve fares information."@en ;
                            rdfs:label "fareAuthorityFilter"@en .


###  https://w3id.org/mobility/trias/fares#fareParam
trfares:fareParam rdf:type owl:DatatypeProperty .


###  https://w3id.org/mobility/trias/fares#fareZoneRef
trfares:fareZoneRef rdf:type owl:DatatypeProperty ;
                    rdfs:domain trfares:FareZone ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Code for a tariff zone."@en ;
                    rdfs:label "fareZoneRef"@en .


###  https://w3id.org/mobility/trias/fares#fareZoneText
trfares:fareZoneText rdf:type owl:DatatypeProperty ;
                     rdfs:domain trfares:FareZone ;
                     rdfs:range xsd:string ;
                     rdfs:label "Name of tariff zone for passengers."@en ,
                                "fareZoneText"@en .


###  https://w3id.org/mobility/trias/fares#faresAuthorityRef
trfares:faresAuthorityRef rdf:type owl:DatatypeProperty ;
                          rdfs:domain trfares:FareZoneListInArea ;
                          rdfs:range xsd:string ;
                          rdfs:comment "Code for a fare authority or a company fare."@en ;
                          rdfs:label "faresAuthorityRef"@en .


###  https://w3id.org/mobility/trias/fares#faresAuthorityText
trfares:faresAuthorityText rdf:type owl:DatatypeProperty ;
                           rdfs:domain trfares:FareZoneListInArea ,
                                       trfares:Ticket ;
                           rdfs:range xsd:string ;
                           rdfs:comment "Description or name of fare authority."@en ,
                                        "Textual description or name of fares authority."@en ;
                           rdfs:label "faresAuthorityText"@en .


###  https://w3id.org/mobility/trias/fares#fromTripIdRef
trfares:fromTripIdRef rdf:type owl:DatatypeProperty ;
                      rdfs:domain trfares:TripTicketReference ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Identifies the \"valid from\" trip."@en ;
                      rdfs:label "fromTripIdRef"@en .


###  https://w3id.org/mobility/trias/fares#fromTripLegIdRef
trfares:fromTripLegIdRef rdf:type owl:DatatypeProperty ;
                         rdfs:domain trfares:TripFaresResult ;
                         rdfs:range xsd:string ;
                         rdfs:comment "Identifies the \"valid from\" trip leg."@en ;
                         rdfs:label "fromTripLegIdRef"@en .


###  https://w3id.org/mobility/trias/fares#infoUrl
trfares:infoUrl rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:string .


###  https://w3id.org/mobility/trias/fares#netPrice
trfares:netPrice rdf:type owl:DatatypeProperty ;
                 rdfs:domain trfares:Ticket ;
                 rdfs:range xsd:float ;
                 rdfs:comment "Net ticket price as decimal number for calculation purposes."@en ;
                 rdfs:label "netPrice"@en .


###  https://w3id.org/mobility/trias/fares#passengerCategory
trfares:passengerCategory rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf trfares:fareParam ;
                          rdfs:comment "Passenger categories for which to retrieve fares information."@en ;
                          rdfs:label "passengerCategory"@en .


###  https://w3id.org/mobility/trias/fares#phoneNumber
trfares:phoneNumber rdf:type owl:DatatypeProperty ;
                    rdfs:domain trfares:BookingInfo ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Telephone number for booking."@en ;
                    rdfs:label "phoneNumber"@en .


###  https://w3id.org/mobility/trias/fares#price
trfares:price rdf:type owl:DatatypeProperty ;
              rdfs:domain trfares:Ticket ;
              rdfs:range xsd:float ;
              rdfs:comment "Net ticket price as decimal number for calculation purposes."@en ;
              rdfs:label "price"@en .


###  https://w3id.org/mobility/trias/fares#stopPointRef
trfares:stopPointRef rdf:type owl:DatatypeProperty ;
                     rdfs:domain trfares:StopFaresRequest ;
                     rdfs:range xsd:string ;
                     rdfs:label "stopPointRef"@en .


###  https://w3id.org/mobility/trias/fares#tariffLevel
trfares:tariffLevel rdf:type owl:DatatypeProperty ;
                    rdfs:domain trfares:Ticket ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Tariff level (example from Nürnberg “10” or “10+T”)."@en ;
                    rdfs:label "tariffLevel"@en .


###  https://w3id.org/mobility/trias/fares#ticketName
trfares:ticketName rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Name of ticket."@en ;
                   rdfs:label "ticketName"@en .


###  https://w3id.org/mobility/trias/fares#ticketRef
trfares:ticketRef rdf:type owl:DatatypeProperty ;
                  rdfs:label "ticketRef"@en .


###  https://w3id.org/mobility/trias/fares#toTripIdRef
trfares:toTripIdRef rdf:type owl:DatatypeProperty ;
                    rdfs:domain trfares:TripTicketReference ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Identifies the \"valid to\" trip."@en ;
                    rdfs:label "toTripIdRef"@en .


###  https://w3id.org/mobility/trias/fares#toTripLegIdRef
trfares:toTripLegIdRef rdf:type owl:DatatypeProperty ;
                       rdfs:domain trfares:TripFaresResult ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Identifies the \"valid to\" trip leg."@en ;
                       rdfs:label "toTripLegIdRef"@en .


###  https://w3id.org/mobility/trias/fares#travelClass
trfares:travelClass rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf trfares:fareParam ;
                    rdfs:comment "Travel class for which to retrieve tickets."@en ;
                    rdfs:label "travelClass"@en .


###  https://w3id.org/mobility/trias/fares#travellerCard
trfares:travellerCard rdf:type owl:DatatypeProperty ;
                      rdfs:domain trfares:FaresPassenger ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Traveller card that the passenger can make use of."@en ;
                      rdfs:label "travellerCard"@en .


###  https://w3id.org/mobility/trias/fares#validityDuration
trfares:validityDuration rdf:type owl:DatatypeProperty ;
                         rdfs:domain trfares:Ticket ;
                         rdfs:range xsd:string ;
                         rdfs:comment "Maximum validity duration of the ticket after purchase or validation."@en ;
                         rdfs:label "validityDuration"@en .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/mobility/trias/common#ErrorMessage
trcommon:ErrorMessage rdf:type owl:Class .


###  https://w3id.org/mobility/trias/fares#BookingInfo
trfares:BookingInfo rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty trfares:bookingAgencyName ;
                                      owl:someValuesFrom <https://w3id.org/mobility/trias/utility#InternationalText>
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty trfares:bookingDeadline ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:dateTime
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty trfares:bookingUrl ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:string
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty trfares:infoUrl ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:string
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty trfares:phoneNumber ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:string
                                    ] ;
                    rdfs:comment "Information of a booking opportunity."@en ;
                    rdfs:label "BookingInfo"@en .


###  https://w3id.org/mobility/trias/fares#FareZone
trfares:FareZone rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty trfares:fareZoneRef ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange xsd:string
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty trfares:fareZoneText ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange xsd:string
                                 ] ;
                 rdfs:comment "Model of a fare zone."@en ;
                 rdfs:label "FareZone"@en .


###  https://w3id.org/mobility/trias/fares#FareZoneListInArea
trfares:FareZoneListInArea rdf:type owl:Class ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty trfares:hasFareZone ;
                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass trfares:FareZone
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty trfares:faresAuthorityRef ;
                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onDataRange xsd:string
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty trfares:faresAuthorityText ;
                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onDataRange xsd:string
                                           ] ;
                           rdfs:comment "List of fare zones within the area of a Fares Authority."@en ;
                           rdfs:label "FareZoneListInArea"@en .


###  https://w3id.org/mobility/trias/fares#FaresParam
trfares:FaresParam rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty trfares:hasTraveller ;
                                     owl:someValuesFrom trfares:FaresPassenger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty trfares:fareAuthorityFilter ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:string
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty trfares:passengerCategory ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:string
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty trfares:travelClass ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:string
                                   ] ;
                   rdfs:comment "Parameters for fares request."@en ;
                   rdfs:label "FaresParam"@en .


###  https://w3id.org/mobility/trias/fares#FaresPassenger
trfares:FaresPassenger rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty trfares:age ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty trfares:travellerCard ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:string
                                       ] ;
                       rdfs:comment "Structure of a passenger profile for fares calculation."@en ;
                       rdfs:label "FaresPassenger"@en .


###  https://w3id.org/mobility/trias/fares#FaresRequest
trfares:FaresRequest rdf:type owl:Class ;
                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty trfares:hasParams ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass trfares:FaresParam
                                     ] ;
                     rdfs:comment "Fares request structure."@en ;
